{
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "dependsOn": ["build"],
      "env": ["MONGODB_URI", "PORT", "BACKEND_URL", "FRONTEND_URL", "VITE_URL"]
    },
    "build": {
      "dependsOn": ["^build"],
      "env": ["MONGODB_URI", "PORT", "BACKEND_URL", "FRONTEND_URL", "VITE_URL"]
    }
  }
}
